	    	// pagination: '.swiper-pagination',
	        pagination: '.swiper-pagination',
	        scrollbar: '.swiper-scrollbar',
	        paginationClickable: true,
	        nextButton: '.swiper-button-next',
	        prevButton: '.swiper-button-prev',
	        parallax: true,
	        speed: 600,
	        loop:true,
	        autoplay:1500,
	    });

	    var swiper2 = new Swiper('#sw2', {
        effect: 'cube',
	    // scrollbar: '.swiper-scrollbar',
        loop:true,
        autoplay:2000,
        speed:800,
        cube: {
            shadow: true,
            slideShadows: true,
            shadowOffset: 20,
            shadowScale: 0.94,
        	}
    	});
    </script>
</body>
</html>
